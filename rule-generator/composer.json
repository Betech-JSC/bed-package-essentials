{
    "name": "jamstackvietnam/rule-generator",
    "require": {
        "doctrine/dbal": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1@dev",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Jamstackvietnam\\RuleGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Models\\": "tests/models"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
